generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
}

model chatbot_faq {
    id        String   @id @default(cuid())
    question  String
    answer    String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
